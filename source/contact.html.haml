---
title: Contact us
keywords: contact, form
skip_side_menu: true
---



.row
  .col-sm-12
    %div{:role => "tabpanel"}
      / Nav tabs
      %ul.nav.nav-pills{:role => "tablist"}
        %li.nav-item{:role => "presentation"}
          %a.nav-link.active{"aria-controls" => "form-tab", "data-toggle" => "tab", :href => "#form-tab", :role => "tab"} 
            .fa.fa-paper-plane
            Contact Form
        %li.nav-item{:role => "presentation"}
          %a.nav-link{"aria-controls" => "email-tab", "data-toggle" => "tab", :href => "#email-tab", :role => "tab"} 
            .fa.fa-envelope
            Email
        %li.nav-item{:role => "presentation"}
          %a.nav-link{"aria-controls" => "phone-tab", "data-toggle" => "tab", :href => "#phone-tab", :role => "tab"} 
            .fa.fa-phone
            Phone      
        %li.nav-item{:role => "presentation"}
          %a.nav-link{"aria-controls" => "hours-tab", "data-toggle" => "tab", :href => "#hours-tab", :role => "tab"} 
            .fa.fa-calendar
            Hours             
        %li.nav-item{:role => "presentation"}
          %a.nav-link{"aria-controls" => "location-tab", "data-toggle" => "tab", :href => "#location-tab", :role => "tab"} 
            .fa.fa-location-arrow
            Location

      / Tab panes
      .tab-content
        #form-tab.tab-pane.active{:role => "tabpanel"}= partial('partials/site/contact/form')
        #email-tab.tab-pane.fade{:role => "tabpanel"}= partial('partials/site/contact/email')
        #phone-tab.tab-pane.fade{:role => "tabpanel"}= partial('partials/site/contact/phone')
        #hours-tab.tab-pane.fade{:role => "tabpanel"}= partial('partials/site/contact/hours')
        #location-tab.tab-pane.fade{:role => "tabpanel"}= partial('partials/site/contact/location')


= partial('partials/site/contact/javascript')




